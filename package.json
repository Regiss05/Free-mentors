{
  "name": "free-mentors",
  "version": "0.0.0",
  "private": true,
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "sourcemap": false,
    "instrument": true
  },
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js",
    "start-dev": "nodemon --exec babel-node ./server/app.js",
    "dev": "cross-env NODE_ENV=development nodemon ./server/app.js",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "test": "cross-env NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit server/v2/tests/*js"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^5.2.1",
    "debug": "~2.6.9",
    "dotenv": "^8.1.0",
    "express": "~4.16.1",
    "heroku": "^7.29.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "~1.9.1",
    "nyc": "^14.1.1",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "eslint": "^6.2.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.14.3",
    "nodemon": "^1.19.1"
  }
}
